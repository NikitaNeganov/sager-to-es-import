{
  "parser": "babel-eslint",
  "extends": "airbnb/base",
  "rules": {
    "linebreak-style": "off",
    "import/prefer-default-export": [
      0
    ]
  },
  "env": {
    "browser": true,
    "jest": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": [
          "src"
        ]
      }
    },
    "ecmascript": 6
  }
}
